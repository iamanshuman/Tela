<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.tela">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tela_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tela_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tela">
        <activity
            android:name="Poll.PollActivity"
            android:exported="false" />
        <activity android:name="admin.AdminActivity" />
        <activity android:name=".initial">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="memes.MemesActivity" />
        <activity android:name="news.webView" />
        <activity android:name="news.Home" />
    </application>

</manifest>